    "c=np.array([-2,-3,5]) # 目标函数系数,3 *1 列向量\n",
    "A=np.array([[-2,5,-1],[1,3,1]]) # 不等式约束系数，A2维矩阵*3\n",
    "b=np.array([-10,12]) # 等式约束系数b, 维列向量2*1\n",
    "A_eq=np.array([[1,1,1]]) # 等式约束系数，Aeq3维列向量*1\n",
    "b_eq=np.array([7]) # 等式约束系数，beq1数值*\n",
